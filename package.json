{
  "name": "mbed-cloud-sdk",
  "version": "2.0.1",
  "is_published": true,
  "is_locked": false,
  "description": "Mbed Cloud SDK for JavaScript",
  "homepage": "https://www.mbed.com",
  "author": "Rob Moran <rob.moran@arm.com>",
  "license": "Apache-2.0",
  "types": "./types/index.d.ts",
  "main": "./index.js",
  "browser": {
    "./index.js": "./bundles/index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ARMmbed/mbed-cloud-sdk-javascript.git"
  },
  "keywords": [
    "Mbed",
    "SDK"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./{lib,bundles,types} && mkdir -p ./{lib,bundles,types} && rm -rf ./coverage",
    "clean:integration": "rm -rf test/integration/build && mkdir test/integration/build && cp package.json test/integration/build",
    "clean:unit": "rm -rf test/unit/{build,types} && mkdir test/unit/build && cp package.json test/unit/build",
    "lint": "tslint -c tslint.json 'src/**/.ts'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/.ts'",
    "docs": "rm -rf ./docs/_docs && mkdir -p ./docs/_docs && typedoc --options typedoc.json ./src/**/*.ts",
    "compile": "npm run clean && tsc",
    "integration:watch": "npm run clean:server && tsc-watch -p integration.tsconfig.json --onSuccess \"node test/integration/build/test/integration/codeCoverage.js\"",
    "integration": "npm run clean:integration && tsc -p integration.tsconfig.json && node test/integration/build/test/integration/codeCoverage.js",
    "build": "npm run clean && npm run lint:fix && npm run docs && npm run compile && npm run gulp",
    "start": "node examples/proxy/server.js",
    "gulp": "gulp",
    "tpip": "node scripts/generate-tpip.js",
    "snippets": "intern config=@snippets",
    "generate": "node generator/build.js && tsfmt -r ./src/sdk/generated/**/**/*.ts ./src/sdk/generated/*.ts && gulp",
    "test": "npm run clean:unit && tsc -p unit.tsconfig.json && intern",
    "test:watch": "npm run clean:unit && tsc-watch -p unit.tsconfig.json --onSuccess \"intern\""
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/memory-cache": "^0.2.0",
    "@types/node": "^7.0.67",
    "@types/superagent": "^2.0.36",
    "body-parser": "^1.18.2",
    "browserify": "^14.0.0",
    "csv-write-stream": "2.0.0",
    "del": "^2.2.2",
    "ejs": "^2.6.1",
    "express": "^4.14.1",
    "gulp": "^3.9.1",
    "gulp-buffer": "0.0.2",
    "gulp-changed": "^3.2.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-tap": "^0.1.3",
    "gulp-tslint": "^8.1.2",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.1.6",
    "gulp-uglify": "^2.0.1",
    "intern": "4.1.2",
    "memory-cache": "^0.2.0",
    "merge2": "^1.0.3",
    "nlf": "^2.0.0",
    "nyc": "^11.3.0",
    "rimraf": "2.6.2",
    "source-map-merger": "~0.2.0",
    "tsc-watch": "^1.0.26",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typedoc": "^0.11.1",
    "typescript": "~2.6.2",
    "typescript-formatter": "^7.2.2",
    "typescript-logging": "^0.5.0"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "superagent": "^3.4.1"
  }
}
