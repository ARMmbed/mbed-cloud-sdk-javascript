{
  "name": "mbed-cloud-sdk",
  "version": "2.0.1",
  "is_published": true,
  "is_locked": false,
  "description": "Mbed Cloud SDK for JavaScript",
  "homepage": "https://www.mbed.com",
  "author": "Rob Moran <rob.moran@arm.com>",
  "license": "Apache-2.0",
  "types": "./types/index.d.ts",
  "main": "./index.js",
  "files": [
    "bundles",
    "lib",
    "examples"
  ],
  "browser": {
    "./index.js": "./bundles/index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ARMmbed/mbed-cloud-sdk-javascript.git"
  },
  "keywords": [
    "Mbed",
    "SDK"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "npm run lint:fix && npm run compile && npm run bundle",
    "bundle": "gulp",
    "clean": "rm -rf ./lib && rm -rf ./bundles && rm -rf ./types",
    "clean:integration": "rm -rf test/integration/build && mkdir test/integration/build && cp package.json test/integration/build && rm -rf ./coverage",
    "clean:unit": "rm -rf test/unit/build && mkdir test/unit/build && cp package.json test/unit/build && rm -rf ./coverage",
    "compile": "tsc",
    "compile:unit": "tsc -p unit.tsconfig.json",
    "compile:integration": "tsc -p integration.tsconfig.json",
    "docs": "rm -rf ./docs/_docs && mkdir -p ./docs/_docs && typedoc --options typedoc.json ./src/**/*.ts",
    "format": "prettier --write --tab-width 4 --print-width 120 --trailing-comma es5 './src/sdk/generated/**/**/*.ts' './src/sdk/generated/*.ts'",
    "generate": "node generator/build.js && npm run format && npm run lint:fix && npm run compile",
    "integration": "npm run compile:integration && node test/integration/build/test/integration/codeCoverage.js",
    "integration:watch": "tsc-watch -p integration.tsconfig.json --onSuccess \"node test/integration/build/test/integration/codeCoverage.js\"",
    "lint": "tslint -c tslint.json 'src/**/**/*.ts'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/**/*.ts'",
    "start-proxy": "node examples/proxy/server.js",
    "test": "npm run compile:unit && intern",
    "test:browser": "npm run compile:unit && npm run bundle && intern",
    "test:snippets": "npm run compile:unit && intern config=@snippets",
    "test:watch": "tsc-watch -p unit.tsconfig.json --onSuccess \"intern\"",
    "tpip": "node scripts/generate-tpip.js"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/express": "^4.0.37",
    "@types/memory-cache": "^0.2.0",
    "@types/node": "^7.0.67",
    "@types/superagent": "^2.0.36",
    "body-parser": "^1.18.2",
    "browserify": "^14.0.0",
    "chai": "^4.2.0",
    "csv-write-stream": "2.0.0",
    "del": "^2.2.2",
    "ejs": "^2.6.1",
    "express": "^4.14.1",
    "fs-extra": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-buffer": "0.0.2",
    "gulp-changed": "^3.2.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-tap": "^0.1.3",
    "gulp-tslint": "^8.1.2",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.1.6",
    "gulp-uglify": "^2.0.1",
    "intern": "4.1.2",
    "memory-cache": "^0.2.0",
    "merge2": "^1.0.3",
    "nlf": "^2.0.0",
    "nyc": "^11.3.0",
    "prettier": "^1.14.3",
    "rimraf": "2.6.2",
    "source-map-merger": "~0.2.0",
    "tsc-watch": "^1.0.26",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typedoc": "^0.11.1",
    "typescript": "~2.6.2",
    "typescript-formatter": "^7.2.2",
    "typescript-logging": "^0.5.0"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "superagent": "^3.4.1"
  }
}
