import { Config } from "../client/config";
<% entities.forEach(function(entity){ %>
    <% if (entity.modes.length > 1) { %>
        import { <%= entity._key.pascal %> } from "./accounts/<%= entity._key.lower_camel %>/<%= entity._key.lower_camel %>";
    <% } %>
<%});%>

export class Factory {
private readonly _config: Config;
constructor(config: Config) {
this._config = config;
}
<% entities.forEach(function(entity){ %>
    <% if (entity.modes.length > 1) { %>
        public <%= entity._key.pascal %>(): <%= entity._key.pascal %> {
        return new <%= entity._key.pascal %>(this._config);
        }
    <% } %>
<%});%>
}
