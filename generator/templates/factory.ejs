import { Config } from "../client/config";
<% entities.forEach(function(entity){ %>
    <% if (entity.methods.length > 1) { %>
        import { <%= snakeToPascal(entity._key) %> } from "./index";
    <% } %>
<%});%>

export class Factory {
private readonly _config: Config;
constructor(config: Config) {
this._config = config;
}
<% entities.forEach(function(entity){ %>
    <% if (entity.methods.length > 1) { %>
        public <%= snakeToPascal(entity._key) %>(): <%= snakeToPascal(entity._key) %> {
        return new <%= snakeToPascal(entity._key) %>(this._config);
        }
    <% } %>
<%});%>
}
