{
    "out": "docs/docs",
    "mode": "file",
    "include": [
        "src/**"
    ],
    "exclude": [
        "node_modules",
        "src/legacy/_api/**/*.ts",
        "src/schema/**/*.ts",
        "src/foundation/_schemas/**/*.ts"
    ],
    "excludeExternals": true,
    "excludePrivate": true,
    "module": "commonjs",
    "target": "es6",
    "name": "Pelion Device Management SDK for JavaScript",
    "readme": "docs/documentation.md",
    "theme": "docs/theme",
    "ignoreCompilerErrors": true
}